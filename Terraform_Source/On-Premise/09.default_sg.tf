resource "aws_default_security_group" "ON-PREMISE-default" {
  vpc_id = aws_vpc.ON-PREMISE-VPC.id

  ingress {
    protocol  = -1
    self      = true
    from_port = 0
    to_port   = 0
  }

  egress {
    from_port   = 0
    to_port     = 0
    protocol    = "-1"
    cidr_blocks = ["0.0.0.0/0"]
  }
}